<h1>Listing user_details</h1>

<table>
  <tr>
    <th>Summary</th>
    <th>Phone</th>
    <th>View count</th>
    <th>Thumbnail</th>
    <th>Website1</th>
    <th>Website2</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_details.each do |user_detail| %>
  <tr>
    <td><%= user_detail.summary %></td>
    <td><%= user_detail.phone %></td>
    <td><%= user_detail.view_count %></td>
    <td><%= user_detail.thumbnail %></td>
    <td><%= user_detail.website1 %></td>
    <td><%= user_detail.website2 %></td>
    <td><%= link_to 'Show', user_detail %></td>
    <td><%= link_to 'Edit', edit_user_detail_path(user_detail) %></td>
    <td><%= link_to 'Destroy', user_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

