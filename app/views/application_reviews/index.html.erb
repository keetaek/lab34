<h1>Listing application_reviews</h1>

<table>
  <tr>
    <th>Score</th>
    <th>Comment</th>
    <th>Application</th>
    <th>Audition admin</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @application_reviews.each do |application_review| %>
  <tr>
    <td><%= application_review.score %></td>
    <td><%= application_review.comment %></td>
    <td><%= application_review.application_id %></td>
    <td><%= application_review.audition_admin_id %></td>
    <td><%= link_to 'Show', application_review %></td>
    <td><%= link_to 'Edit', edit_application_review_path(application_review) %></td>
    <td><%= link_to 'Destroy', application_review, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Application review', new_application_review_path %>
